<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Trexler Class Library</Description>
    <Authors>golf1052</Authors>
    <TargetFrameworks>net451;netstandard1.3</TargetFrameworks>
    <AssemblyName>golf1052.Trexler</AssemblyName>
    <PackageId>golf1052.Trexler</PackageId>
    <PackageProjectUrl>https://github.com/golf1052/Trexler</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/golf1052/Trexler/blob/master/LICENSE</PackageLicenseUrl>
    <NetStandardImplicitPackageVersion Condition=" '$(TargetFramework)' == 'netstandard1.3' ">1.6.0</NetStandardImplicitPackageVersion>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net451' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
